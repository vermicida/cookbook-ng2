{
  "name": "cookbook-ng2",
  "version": "0.0.1",
  "description": "Recetario de cocina desarrollado en Angular 2.",
  "author": "Diego Herrera",
  "homepage": "https://github.com/vermicida/cookbook-ng2#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vermicida/cookbook-ng2.git"
  },
  "bugs": {
    "url": "https://github.com/vermicida/cookbook-ng2/issues"
  },
  "scripts": {
    "clean": "rm -f ./app/*.js; rm -f ./app/*.js.map;",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "sparrest": "node ./node_modules/node-sparrest/node-sparrest.js",
    "start": "tsc && concurrently \"npm run sparrest\" \"npm run tsc:w\" \"npm run lite\""
  },
  "keywords": [
    "angular",
    "angular2",
    "cookbook"
  ],
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.11",
    "es6-shim": "^0.35.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "skeleton-css": "^2.0.4",
    "systemjs": "0.19.29",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "lite-server": "^2.2.0",
    "node-sparrest": "^0.0.10",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
